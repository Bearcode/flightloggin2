<fb:header>
    Welcome, <fb:name uid="{{ uid }}" firstnameonly="true" useyou="false" />!
</fb:header>

{% if not message %}

    <form method="post" action=".">
    <input type="text" name="username"> <label for="username">Username</label><br>
    <input type="text" name="secret_key"> <label for="secret_key">Secret Key</label><br>
    <input type="submit" value="Submit">
    </form>

{% else %}
    {{message}}
{% endif %}
