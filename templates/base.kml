<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>{{title}}</name>
	
	<Style id="purple_line">
		<LineStyle>
			<color>ccFF78B4</color> {# B478FF #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="blue_line">
		<LineStyle>
			<color>ccF02814</color> {# 1428F0 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="red_line">
		<LineStyle>
			<color>cc1400D2</color> {# D20014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="orange_line">
		<LineStyle>
			<color>cc14B1FF</color>  {# FFB114 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="green_line">
		<LineStyle>
			<color>cc14F000</color> {# 00F014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="base_icon">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>{{ MEDIA_URL }}/icons/big/base_pad.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>

	<Style id="route_icon">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>{{ MEDIA_URL }}/icons/small/base_pad.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	
	
    {% for folder in route_folders %}
    <Folder>
    <name>{{folder.name}}</name>
	    {% for route in folder %}
	    <Placemark>
		    <name>{{route.name}}</name>
		    <styleUrl>{{folder.style}}</styleUrl>
		    <LineString>
			    <tessellate>1</tessellate>		
			    <coordinates>
			    {{route.kml}}
			    </coordinates>
		    </LineString>
	    </Placemark>
	    {% endfor %}
	</Folder>
	{% endfor %}
	
	{% for folder in point_folders %}
	<Folder>
	<name>{{folder.name}}</name>
        {% for point in bases %}
	    <Placemark>
		    <name>{{point.identifier}}</name>
		    <description>
		    <![CDATA[
                    <b>{{point.name}}</b><br>{{point.location_summary}}
            ]]>
            </description>
		    <styleUrl>#base_icon</styleUrl>
		    <Point>
			    <coordinates>{{point.location.x}},{{point.location.y}},0</coordinates>
		    </Point>
	    </Placemark>
	    {% endfor %}
	</Folder>
	{% endfor %}
		
</Document>
</kml>
