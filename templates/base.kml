<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>{{title}}</name>
	
	<Style id="purple_line">
		<LineStyle>
			<color>50780078</color> {# 780078 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="blue_line">
		<LineStyle>
			<color>50FF7800</color> {# 0078FF #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="red_line">
		<LineStyle>
			<color>501400D2</color> {# D20014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="orange_line">
		<LineStyle>
			<color>5014B1FF</color>  {# FFB114 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="green_line">
		<LineStyle>
			<color>5014F000</color> {# 00F014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="base_icon">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>{{ MEDIA_URL }}/icons/big/base_pad.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>

	<Style id="route_icon">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>{{ MEDIA_URL }}/icons/small/base_pad.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	
	
    {% for folder in folders %}
    <Folder>
    <name>{{folder.name}}</name>
	    {% for route in folder %}
	    <Placemark>
		    <name>{{route.name}}</name>
		    <styleUrl>{{folder.style}}</styleUrl>
		    <LineString>
			    <tessellate>1</tessellate>		
			    <coordinates>
			    {{route.kml}}
			    </coordinates>
		    </LineString>
	    </Placemark>
	    {% endfor %}
	</Folder>
	{% endfor %}
	
	
	
	
	
	
	{% for point in routebases %}
	<Placemark>
		<name>{{point.identifier}}</name>
		<description>
		<![CDATA[
                <b>{{point.name}}</b><br>{{point.location_summary}}
        ]]>
        </description>
		<styleUrl>#route_icon</styleUrl>
		<Point>
			<coordinates>{{point.location.x}},{{point.location.y}},0</coordinates>
		</Point>
	</Placemark>
	{% endfor %}
	
	
	
	
	
	{% for point in bases %}
	<Placemark>
		<name>{{point.identifier}}</name>
		<description>
		<![CDATA[
                <b>{{point.name}}</b><br>{{point.location_summary}}
        ]]>
        </description>
		<styleUrl>#base_icon</styleUrl>
		<Point>
			<coordinates>{{point.location.x}},{{point.location.y}},0</coordinates>
		</Point>
	</Placemark>
	{% endfor %}
	
	
	
	
</Document>
</kml>
