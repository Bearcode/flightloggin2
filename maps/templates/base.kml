<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" xmlns:gx="http://www.google.com/kml/ext/2.2" xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
	<name>{{title}}</name>

	<Style id="purple_line">
		<LineStyle>
			<color>ccFF78B4</color>{# B478FF #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="blue_line">
		<LineStyle>
			<color>ccF02814</color>{# 1428F0 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="red_line">
		<LineStyle>
			<color>cc1400D2</color>{# D20014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="orange_line">
		<LineStyle>
			<color>cc14B1FF</color>{# FFB114 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	<Style id="green_line">
		<LineStyle>
			<color>cc14F000</color>{# 00F014 #}
			<width>2</width>
		</LineStyle>
	</Style>
	
	{##########################################}
	
	<Style id="yellow">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/yellow.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="orange">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/orange.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="red">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/red.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="green">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/green.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="purple">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/purple.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="cyan">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/cyan.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="teal">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/teal.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
	
	<Style id="gray">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/gray.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>

	<Style id="white">
		<IconStyle>
			<scale>1.0</scale>
			<Icon>
				<href>files/white.png</href>
			</Icon>
			<hotSpot x="0.5" y="0.5" xunits="fraction" yunits="fraction"/>
		</IconStyle>
	</Style>
    
    {##########################################}
		
{% for folder in folders %}	

    {% if folder.has_routes %}
        <Folder>
        <name>{{folder.name}}</name>
	        {% for route in folder %}
	        <Placemark>
		        <name>{{route.name}}</name>
		        <styleUrl>{{folder.style}}</styleUrl>
		        <description>
		        {% url profile-route route.name as url %}
		        <![CDATA[<a href="http://flightlogg.in{{ url }}">
		                 See who has flown this route
		                 </a>
		                 ]]>
            </description>
		        <LineString>
			        <tessellate>1</tessellate>		
			        <coordinates>
			        {{route.kml}}
			        </coordinates>
		        </LineString>
	        </Placemark>
	        {% endfor %}
	    </Folder>
    {% endif %}

    {% if folder.has_points %}
	<Folder>
	<name>{{folder.name}}</name>
        {% for point in folder %}
	    <Placemark>
		    <name>{{point.identifier}}</name>
		    <styleUrl>{{ point.icon }}</styleUrl>
		    {% url profile-location point.identifier as url %}
		    <description>
		        <![CDATA[<b><a href="http://flightlogg.in{{ url }}">
		        {{ point.name }}</a></b>
		        <br>{{point.ls}}
		        ]]>
            </description>
		    <Point>
			    <coordinates>{{point.kml}}</coordinates>
		    </Point>
	    </Placemark>
	    {% endfor %}
	</Folder>
    {% endif %}
    
{% endfor %}
</Document>
</kml>
